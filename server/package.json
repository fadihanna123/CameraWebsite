    "@prisma/client": "^6.2.1",
    "express": "^4.21.2",
    "express-fileupload": "^1.5.1",
    "helmet": "^8.0.0",
    "jsonwebtoken": "^9.0.2",
    "luxon": "^3.5.0",
    "validator": "^13.12.0",
    "winston": "^3.17.0"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": "Please use Yarn instead of NPM to install dependencies"
  },
  "config": {
