{
  "name": "surveillance_server",
  "version": "1.0.57",
  "lastUpgrade": "20251007",
  "homepage": "https://localhost:5000",
  "license": "MIT",
  "private": "true",
  "bugs": "https://github.com/fadihanna123/CameraWebsite/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/fadihanna123/CameraWebsite/tree/main/server"
  },
  "author": {
    "name": "Fadi Hanna"
  },
  "scripts": {
    "setup": "yarn --ignore-scripts --silent --emoji --immutable",
    "dev": "ts-node-dev --poll -r tsconfig-paths/register ./src/server",
    "seed": "prisma db push && prisma db pull && prisma generate",
    "studio": "prisma studio",
    "seed-delete": "prisma migrate reset",
    "lint": "eslint && tsc --noEmit",
    "format": "prettier -w ./",
    "prebuild": "rimraf build",
    "build": "esbuild ./src/server.ts --minify --bundle --platform=node --outfile=build/server.js",
    "start": "node ./build/server.js",
    "clear": "rimraf ./node_modules ./build"
  },
  "devDependencies": {
    "@types/cors": "2.8.19",
    "@types/eslint": "9.6.1",
    "@types/express": "5.0.3",
    "@types/express-fileupload": "1.5.1",
    "@types/jsonwebtoken": "9.0.10",
    "@types/luxon": "3.7.1",
    "@types/node-cron": "3.0.11",
    "@types/validator": "13.15.3",
    "@typescript-eslint/eslint-plugin": "8.46.0",
    "@typescript-eslint/parser": "8.46.0",
    "cors": "2.8.5",
    "dotenv": "17.2.3",
    "esbuild": "^0.25.12",
    "eslint": "9.37.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-prettier": "5.5.4",
    "express-rate-limit": "8.1.0",
    "node-cron": "4.2.1",
    "prettier": "3.6.2",
    "prisma": "6.17.0",
    "rimraf": "6.0.1",
    "ts-node-dev": "2.0.0",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.9.3"
  },
  "dependencies": {
    "@prisma/client": "6.17.0",
    "bcryptjs": "^3.0.2",
    "express": "5.1.0",
    "express-fileupload": "1.5.2",
    "helmet": "8.1.0",
    "jsonwebtoken": "9.0.2",
    "luxon": "3.7.2",
    "validator": "13.15.15",
    "winston": "3.18.3"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": "Please use Yarn instead of NPM to install dependencies"
  },
  "config": {
    "port": 5000
  }
}
