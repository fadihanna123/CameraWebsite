  },
  "bugs": "https://github.com/fadihanna123/CameraWebsite/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/fadihanna123/CameraWebsite/tree/main/client"
  },
  "license": "MIT",
  "homepage": "./",
  "private": "true",
  "dependencies": {
    "@types/react-dom": "^19.0.2",
    "axios": "1.7.9",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.1.1",
    "react-scripts": "5.0.1",
    "react-toastify": "^11.0.2",
    "redux": "^5.0.1",
    "sass": "^1.83.0"
  },
  "scripts": {
    "setup": "yarn --ignore-scripts --silent --emoji --immutable",
    "dev": "react-scripts start",
    "prebuild": "rm -rf build dist",
    "build": "yarn prebuild && set \"GENERATE_SOURCEMAP=false\" && react-scripts build",
    "test": "jest",
    "eject": "react-scripts eject",
    "lint": "eslint . && tsc",
    "format": "prettier -w .",
    "preview": "yarn build && serve build",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "clear": "rimraf node_modules dist build",
    "ci": "yarn build && yarn lint && rimraf dist build"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.17.0",
    "@reduxjs/toolkit": "^2.5.0",
    "@storybook/addon-essentials": "8.4.7",
    "@storybook/addon-interactions": "8.4.7",
    "@storybook/addon-links": "8.4.7",
    "@storybook/preset-create-react-app": "8.4.7",
    "@storybook/react": "8.4.7",
    "@storybook/react-webpack5": "8.4.7",
    "@types/eslint": "^9.6.1",
    "@types/jest": "^29.5.14",
    "@types/react": "^19.0.2",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "eslint": "9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.3",
    "eslint-plugin-storybook": "^0.11.1",
    "globals": "^15.14.0",
    "prettier": "^3.4.2",
    "storybook": "8.4.7",
    "stylelint": "^16.12.0",
    "stylelint-config-standard": "^36.0.1",
    "stylelint-config-standard-scss": "^14.0.0",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=6.0.0"
  },
  "eslintConfig": {
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      },
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "config": {
    "port": 3000
  }
}
