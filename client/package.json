    "react-scripts": "5.0.1",
    "react-toastify": "11.0.5",
    "redux": "5.0.1",
    "sass": "1.89.2"
  },
  "scripts": {
    "setup": "yarn --ignore-scripts --silent --emoji --immutable",
    "dev": "vite",
    "prebuild": "rimraf build",
    "build": "yarn prebuild && set \"GENERATE_SOURCEMAP=false\" && vite build",
    "test": "vitest",
    "eject": "react-scripts eject",
    "lint": "eslint . && htmlhint **/*.html && stylelint **/*.css && tsc --noEmit",
    "format": "prettier -w .",
    "preview": "yarn build && vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "ci": "yarn lint && yarn test && yarn build",
    "clear": "npx rimraf node_modules build storybook-static"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@eslint/eslintrc": "3.3.1",
    "@eslint/js": "9.32.0",
    "@reduxjs/toolkit": "2.8.2",
    "@storybook/addon-essentials": "8.6.14",
    "@storybook/addon-interactions": "8.6.14",
    "@storybook/addon-links": "9.1.0",
    "@storybook/builder-vite": "^9.1.0",
    "@storybook/preset-create-react-app": "9.1.0",
    "@storybook/react": "9.1.0",
    "@storybook/react-webpack5": "9.1.0",
    "@testing-library/react": "16.3.0",
    "@types/eslint": "9.6.1",
    "@types/jest": "30.0.0",
    "@types/node": "24.1.0",
    "@types/react": "19.1.9",
    "@types/react-router-dom": "5.3.3",
    "@typescript-eslint/eslint-plugin": "8.38.0",
    "@typescript-eslint/parser": "8.38.0",
    "@vitejs/plugin-react": "4.7.0",
    "eslint": "9.32.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-config-react-app": "7.0.1",
    "eslint-plugin-html": "8.1.3",
    "eslint-plugin-prettier": "5.5.3",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-storybook": "9.1.0",
    "globals": "16.3.0",
    "htmlhint": "1.6.3",
    "prettier": "3.6.2",
    "rimraf": "6.0.1",
    "storybook": "9.1.0",
    "stylelint": "16.23.0",
    "stylelint-config-standard": "39.0.0",
    "stylelint-config-standard-scss": "15.0.1",
    "tailwindcss": "4.1.11",
    "typescript": "5.9.2",
    "vite": "7.0.6",
    "vite-plugin-commonjs": "0.10.4",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.2.4"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=6.0.0"
  },
  "eslintConfig": {
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      },
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
