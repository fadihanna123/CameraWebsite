{
  "name": "surveillance_website_client",
  "version": "0.1.58",
  "author": {
    "name": "Fadi Hanna",
    "email": "fhanna181@gmail.com"
  },
  "bugs": "https://github.com/fadihanna123/CameraWebsite/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/fadihanna123/CameraWebsite/tree/main/client"
  },
  "license": "MIT",
  "homepage": "./",
  "private": "true",
  "dependencies": {
    "@babel/plugin-proposal-private-property-in-object": "7.21.11",
    "@types/react-dom": "19.0.4",
    "axios": "1.7.9",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-redux": "9.2.0",
    "react-router-dom": "7.2.0",
    "react-scripts": "5.0.1",
    "react-toastify": "11.0.5",
    "redux": "5.0.1",
    "sass": "1.85.1"
  },
  "scripts": {
    "setup": "yarn --ignore-scripts --silent --emoji --immutable",
    "dev": "react-scripts start",
    "prebuild": "rimraf build",
    "build": "yarn prebuild && set \"GENERATE_SOURCEMAP=false\" && react-scripts build",
    "test": "jest",
    "eject": "react-scripts eject",
    "lint": "eslint . && htmlhint **/*.html && stylelint **/*.css && tsc --noEmit",
    "format": "prettier -w .",
    "preview": "yarn build && serve build",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "ci": "yarn lint && yarn test && yarn build",
    "clear": "npx rimraf node_modules build storybook-static"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@eslint/eslintrc": "3.3.0",
    "@eslint/js": "9.21.0",
    "@reduxjs/toolkit": "2.6.0",
    "@storybook/addon-essentials": "8.5.8",
    "@storybook/addon-interactions": "8.5.8",
    "@storybook/addon-links": "8.5.8",
    "@storybook/preset-create-react-app": "8.5.8",
    "@storybook/react": "8.5.8",
    "@storybook/react-webpack5": "8.5.8",
    "@types/eslint": "9.6.1",
    "@types/jest": "29.5.14",
    "@types/node": "22.13.5",
    "@types/react": "19.0.10",
    "@types/react-router-dom": "5.3.3",
    "@typescript-eslint/eslint-plugin": "8.25.0",
    "@typescript-eslint/parser": "8.25.0",
    "eslint": "9.21.0",
    "eslint-config-prettier": "10.0.1",
    "eslint-config-react-app": "7.0.1",
    "eslint-plugin-html": "8.1.2",
    "eslint-plugin-prettier": "5.2.3",
    "eslint-plugin-react": "7.37.4",
    "eslint-plugin-storybook": "0.11.3",
    "globals": "16.0.0",
    "htmlhint": "1.1.4",
    "prettier": "3.5.2",
    "rimraf": "6.0.1",
    "storybook": "8.5.8",
    "stylelint": "16.14.1",
    "stylelint-config-standard": "37.0.0",
    "stylelint-config-standard-scss": "14.0.0",
    "tailwindcss": "4.0.8",
    "typescript": "5.7.3"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=6.0.0"
  },
  "eslintConfig": {
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      },
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "config": {
    "port": 3000
  }
}
