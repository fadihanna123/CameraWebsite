{
  "name": "surveillance_website_client",
  "version": "0.1.77",
  "type": "module",
  "lastUpgrade": "20251222",
  "author": {
    "name": "Fadi Hanna"
  },
  "bugs": "https://github.com/fadihanna123/CameraWebsite/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/fadihanna123/CameraWebsite/tree/main/client"
  },
  "license": "MIT",
  "homepage": "./",
  "private": "true",
  "dependencies": {
    "@types/react-dom": "19.2.3",
    "axios": "1.13.2",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-hook-form": "7.69.0",
    "react-redux": "9.2.0",
    "react-router-dom": "7.11.0",
    "react-scripts": "5.0.1",
    "react-spinners": "0.17.0",
    "react-toastify": "11.0.5",
    "redux": "5.0.1",
    "sass": "1.97.1"
  },
  "scripts": {
    "setup": "yarn --ignore-scripts --silent --emoji --immutable",
    "dev": "vite",
    "prebuild": "rimraf build",
    "build": "yarn prebuild && set \"GENERATE_SOURCEMAP=false\" && vite build",
    "eject": "react-scripts eject",
    "lint": "eslint && htmlhint **/*.html && stylelint **/styles/*.css && tsc --noEmit",
    "format": "prettier -w .",
    "preview": "yarn build && vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "ci": "yarn lint && yarn test && yarn build",
    "clear": "npx rimraf node_modules build storybook-static"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@eslint/eslintrc": "3.3.3",
    "@eslint/js": "9.39.2",
    "@reduxjs/toolkit": "2.11.2",
    "@storybook/addon-essentials": "8.6.14",
    "@storybook/addon-interactions": "8.6.14",
    "@storybook/addon-links": "10.1.10",
    "@storybook/builder-vite": "^10.1.10",
    "@storybook/preset-create-react-app": "10.1.10",
    "@storybook/react": "10.1.10",
    "@storybook/react-vite": "^10.1.10",
    "@storybook/react-webpack5": "10.1.10",
    "@testing-library/react": "16.3.1",
    "@types/eslint": "9.6.1",
    "@types/jest": "30.0.0",
    "@types/node": "25.0.3",
    "@types/react": "19.2.7",
    "@types/react-router-dom": "5.3.3",
    "@typescript-eslint/eslint-plugin": "8.50.0",
    "@typescript-eslint/parser": "8.50.0",
    "@vitejs/plugin-react": "5.1.2",
    "eslint": "9.39.2",
    "eslint-config-prettier": "10.1.8",
    "eslint-config-react-app": "7.0.1",
    "eslint-plugin-html": "8.1.3",
    "eslint-plugin-prettier": "5.5.4",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-storybook": "10.1.10",
    "globals": "16.5.0",
    "htmlhint": "1.8.0",
    "prettier": "3.7.4",
    "rimraf": "6.1.2",
    "storybook": "10.1.10",
    "stylelint": "16.26.1",
    "stylelint-config-standard": "39.0.1",
    "stylelint-config-standard-scss": "16.0.0",
    "typescript": "5.9.3",
    "vite": "7.3.0",
    "vite-plugin-commonjs": "0.10.4",
    "vite-tsconfig-paths": "6.0.3"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=6.0.0"
  },
  "eslintConfig": {
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      },
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "config": {
    "port": 3000
  }
}
