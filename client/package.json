  },
  "license": "MIT",
  "homepage": "./",
  "private": "true",
  "dependencies": {
    "@types/react-dom": "19.1.7",
    "axios": "1.11.0",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-hook-form": "7.62.0",
    "react-redux": "9.2.0",
